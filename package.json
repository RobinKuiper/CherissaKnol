{
  "name": "cherissa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 8082",
    "export": "pnpm build && next export -o build",
    "lint": "next lint",
    "analyze": "ANALYZE=true yarn build",
    "docker-build": "docker build . -t robinkuiper/cherissaknol:latest",
    "docker-push": "docker push robinkuiper/cherissaknol:latest",
    "prisma-generate": "pnpm prisma generate",
    "prisma-deploy": "pnpm prisma migrate deploy",
    "prisma-dev": "pnpm prisma migrate dev",
    "prisma-seed": "pnpm prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@fontsource/bebas-neue": "^4.5.7",
    "@fontsource/roboto": "^4.5.5",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/react-paypal-js": "^7.7.0",
    "@prisma/client": "^3.14.0",
    "bcryptjs": "^2.4.3",
    "formik": "^2.2.9",
    "framer-motion": "^6.3.3",
    "global": "^4.4.0",
    "gridstack": "^5.0.0",
    "next": "12.1.6",
    "next-auth": "^4.3.4",
    "nodemailer": "^6.7.5",
    "nprogress": "^0.2.0",
    "pnpm": "^7.1.2",
    "prisma": "^3.14.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.3.1",
    "react-lightgallery": "^0.9.0",
    "react-modal": "^3.15.1",
    "react-spinners": "^0.11.0",
    "sitemap": "^7.1.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "@types/node": "^17.0.34",
    "@types/react": "^18.0.9",
    "autoprefixer": "^10.4.7",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
